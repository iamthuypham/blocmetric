<h1>Your Registered Applications</h1>
<div class="row">
   <div class="col-md-8">
     <% @registered_applications.each do |registered_application| %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
             <small>
             <%= link_to "De-register", registered_application, method: :delete, class: 'btn btn-danger btn-sm'%>
             </small>
             <%= link_to registered_application.name, registered_applications_path %>
             <small>
             <%= registered_application.url %>
             </small>
           </h4>
         </div>
       </div>
     <% end %>
   </div>
  </br>
  <div class="col-md-4">
      <%= link_to "Register New Application", new_registered_application_path, class: 'btn btn-success' %>
  </div>
</div>
